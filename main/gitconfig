        name = "Ari Greenberg"
        email = "arigreenberg@gmail.com"

[pull]
        rebase = false
[push]
        default = simple

# http://stackoverflow.com/questions/18257622/why-is-git-core-preloadindex-default-value-false
[core]
        preloadindex = true

[help]
  # Automatically correct and execute mistyped commands such as `git stauts`
  autocorrect = 1

